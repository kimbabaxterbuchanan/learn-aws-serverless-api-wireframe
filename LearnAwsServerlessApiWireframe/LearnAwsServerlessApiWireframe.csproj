<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <!-- Roll forward to future major versions of the netcoreapp as needed -->
    <RollForward>Major</RollForward>
  </PropertyGroup>

  <ItemGroup>
    <!-- CDK Construct Library dependencies -->
    <PackageReference Include="Amazon.CDK.AWS.APIGateway" Version="1.173.0" />
    <PackageReference Include="Amazon.CDK.AWS.DynamoDB" Version="1.173.0" />
    <PackageReference Include="Amazon.CDK.AWS.S3" Version="1.173.0" />
    <PackageReference Include="Amazon.Lambda.APIGatewayEvents" Version="2.5.0" />
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.5.3" />
    <PackageReference Include="AWSSDK.Rekognition" Version="3.7.10.17" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.9.56" />

    <!-- jsii Roslyn analyzers (un-comment to obtain compile-time checks for missing required props
    <PackageReference Include="Amazon.Jsii.Analyzers" Version="*" PrivateAssets="all" />
    -->
  </ItemGroup>

</Project>
